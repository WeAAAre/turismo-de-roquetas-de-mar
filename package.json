{
  "name": "@gisei-studio/turismo-de-roquetas-de-mar",
  "version": "0.0.0",
  "description": "Template for a mono repo for a Prisma projects",
  "homepage": "https://github.com/gisei-studio/turismo-de-roquetas-de-mar",
  "bugs": {
    "url": "https://github.com/gisei-studio/turismo-de-roquetas-de-mar/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gisei-studio/turismo-de-roquetas-de-mar.git"
  },
  "license": "UNLICENSED",
  "author": "Carlos Garrido Marin (cgm164)",
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean && rimraf .turbo node_modules",
    "dev": "turbo run dev",
    "format": "prettier --cache --check  apps/**/*.{ts,tsx,js,jsx,json,md}",
    "format:fix": "prettier --write  apps/**/*.{ts,tsx,js,jsx,json,md}",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "prepare": "husky install",
    "publish:packages": "changeset version && changeset publish"
  },
  "prettier": "@gisei-studio/style/prettier",
  "devDependencies": {
    "@changesets/cli": "2.27.1",
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@gisei-studio/style": "0.0.0",
    "eslint": "8.56.0",
    "husky": "8.0.3",
    "prettier": "3.2.4",
    "rimraf": "5.0.5",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.9.0",
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  }
}
